@{
    ViewBag.Title = "Customer";
}

<input type="hidden" id="hidSelMenu" value="liPageCustomer" />
<input type="hidden" id="hidSelCustomer" value="000" />

<ul class="breadcrumb">
    <li><a href="#">Home</a></li>
    <li><a href="#">Production</a></li>
    <li class="active">Customers</li>
</ul>
<!-- END BREADCRUMB -->
<!-- PAGE TITLE -->
<div class="page-title">
    <h2><span class="fa fa-arrow-circle-o-left"></span> @ViewBag.Title</h2>
</div>
<!-- END PAGE TITLE -->
<!-- PAGE CONTENT WRAPPER -->
<div class="page-content-wrap">
    <!-- PAGE CONTENT - BEGINS -->
    <div class="row">
        <!-- Customer TABLE, GOVT CODES, OFFICE & OFF PERSONS -->
        <div class="col-md-8" id="divTable">

            <div class="row">
                <!-- CUSTOMER TABLE -->
                <div class="col-md-8">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <button type="button" class="btn btn-primary" id="btnCustNew">New Customer</button>
                            <button type="button" class="btn btn-info" id="btnCustEdit">Edit</button>
                            <button type="button" class="btn btn-danger" id="btnCustDelete">Delete</button>
                            <ul class="panel-controls">
                                <li><a href="#" class="panel-collapse"><span class="fa fa-angle-down"></span></a></li>
                                <li><a href="#" class="panel-refresh"><span class="fa fa-refresh"></span></a></li>
                            </ul>
                        </div>

                        <div class="panel-body">
                            <table class="table DataTable" id="tblCustomer">
                                <thead>
                                    <tr>
                                        <th width="20%">Customer name</th>
                                        <th width="15%">Code</th>
                                        <th width="20%">Website</th>
                                        <th width="25%">Email</th>
                                        <th width="20%">Phone</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="row">
                        <!-- GOVT CODES -->
                        <div class="col-md-12">
                            <div class="panel panel-info">
                                <div class="panel-heading">
                                    <button type="button" class="btn btn-primary" id="btnCodeNew">New Code</button>
                                    <button type="button" class="btn btn-info" id="btnCodeEdit">Edit</button>
                                    <button type="button" class="btn btn-danger" id="btnCodeDelete">Delete</button>
                                    <ul class="panel-controls">
                                        <li><a href="#" class="panel-collapse"><span class="fa fa-angle-down"></span></a></li>
                                        <li><a href="#" class="panel-refresh"><span class="fa fa-refresh"></span></a></li>
                                    </ul>
                                </div>

                                <div class="panel-body">
                                    <table class="table DataTable" id="tblCode">
                                        <thead>
                                            <tr>
                                                <th width="20%">Govt Code</th>
                                                <th width="15%">Type</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <!-- OFFICES  -->
                        <div class="col-md-12">
                            <div class="panel panel-info">
                                <div class="panel-heading">
                                    <button type="button" class="btn btn-primary" id="btnOffNew">New Office</button>
                                    <button type="button" class="btn btn-info" id="btnOffEdit">Edit</button>
                                    <button type="button" class="btn btn-danger" id="btnOffDelete">Delete</button>
                                    <ul class="panel-controls">
                                        <li><a href="#" class="panel-collapse"><span class="fa fa-angle-down"></span></a></li>
                                        <li><a href="#" class="panel-refresh"><span class="fa fa-refresh"></span></a></li>
                                    </ul>
                                </div>

                                <div class="panel-body">
                                    <table class="table DataTable" id="tblOffice">
                                        <thead>
                                            <tr>
                                                <th width="20%">Office title</th>
                                                <th width="20%">Phone</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <!-- OFF PERSONS -->
                        <div class="col-md-12">
                            <div class="panel panel-info">
                                <div class="panel-heading">
                                    <button type="button" class="btn btn-primary" id="btnPersonNew">New Person</button>
                                    <button type="button" class="btn btn-info" id="btnPersonEdit">Edit</button>
                                    <button type="button" class="btn btn-danger" id="btnPersonDelete">Delete</button>
                                    <ul class="panel-controls">
                                        <li><a href="#" class="panel-collapse"><span class="fa fa-angle-down"></span></a></li>
                                        <li><a href="#" class="panel-refresh"><span class="fa fa-refresh"></span></a></li>
                                    </ul>
                                </div>

                                <div class="panel-body">
                                    <table class="table DataTable" id="tblPerson">
                                        <thead>
                                            <tr>
                                                <th width="20%">Name</th>
                                                <th width="20%">Phone</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

        </div>

        <!--Customer EDIT PANELS -->
        <div class="col-md-4" id="divUpdate">

            <div class="panel panel-default" id="divEditCustomer">
                <div class="panel-body">
                    <h4>Customer details</h4>
                    <!-- CUSTOMER form -->
                    <form class="form-horizontal" action="#" role="form" id="frmCustomer">
                        <div class="form-group">
                            <label class="col-md-3 control-label">Customer name</label>
                            <div class="col-md-9">
                                <input type="text" id="txtLogin" class="form-control" placeholder="Customer Name" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Code</label>
                            <div class="col-md-9">
                                <input type="text" id="txtPwd1" class="form-control" placeholder="Code" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Website</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-globe"></i></span>
                                    <input type="text" id="txtFacebook" class="form-control" placeholder="Website" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Email</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-envelope"></i></span>
                                    <input type="text" id="txtEmail" class="form-control" placeholder="Email Address" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Phone</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-phone"></i></span>
                                    <input type="text" id="txtPhone" class="form-control" placeholder="Phone" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Fax</label>
                            <div class="col-md-9">
                                <input type="text" id="txtPwd2" class="form-control" placeholder="Fax" />
                            </div>
                        </div>
                    </form>
                </div>
                <div class="panel-footer">
                    <button id="btnCustUpdateCancel" class="btn btn-link pull-right">Cancel</button>
                    <button id="btnCustUpdateSave" class="btn btn-primary pull-right">Save Changes <span class="fa fa-floppy-o fa-right"></span></button>
                </div>
            </div>

            <div class="panel panel-default" id="divEditCode">
                <div class="panel-body">
                    <h4>Code details</h4>
                    <!-- GOVT CODE form -->
                    <form class="form-horizontal" action="#" role="form" id="frmCode">
                        <div class="form-group">
                            <label class="col-md-3 control-label">Govt code type</label>
                            <div class="col-md-9">
                                <select id="selCountry" class="form-control select"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Govt code</label>
                            <div class="col-md-9">
                                <input type="text" id="txtAddress1" class="form-control" placeholder="Govt code" />
                            </div>
                        </div>
                    </form>
                </div>
                <div class="panel-footer">
                    <button id="btnCustUpdateCancel" class="btn btn-link pull-right">Cancel</button>
                    <button id="btnCustUpdateSave" class="btn btn-primary pull-right">Save Changes <span class="fa fa-floppy-o fa-right"></span></button>
                </div>
            </div>

            <div class="panel panel-default" id="divEditOffice">
                <div class="panel-body">
                    <h4>Office details</h4>
                    <!-- OFFICE form -->
                    <form class="form-horizontal" action="#" role="form" id="frmOffice">
                        <div class="form-group">
                            <label class="col-md-3 control-label">Title</label>
                            <div class="col-md-9">
                                <input type="text" id="txtAddress1" class="form-control" placeholder="Office title" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Address 1</label>
                            <div class="col-md-9">
                                <input type="text" id="txtAddress1" class="form-control" placeholder="Address 1" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Address 2</label>
                            <div class="col-md-9">
                                <input type="text" id="txtAddress2" class="form-control" placeholder="Address 2" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Country</label>
                            <div class="col-md-9">
                                <select id="selCountry" class="form-control select"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">State</label>
                            <div class="col-md-9">
                                <select id="selState" class="form-control select"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">City</label>
                            <div class="col-md-9">
                                <select id="selCity" class="form-control select"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Area</label>
                            <div class="col-md-9">
                                <select id="selArea" class="form-control select"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Email</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-envelope"></i></span>
                                    <input type="text" id="txtEmail" class="form-control" placeholder="Email Address" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Phone</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-phone"></i></span>
                                    <input type="text" id="txtPhone" class="form-control" placeholder="Phone" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Fax</label>
                            <div class="col-md-9">
                                <input type="text" id="txtPwd2" class="form-control" placeholder="Fax" />
                            </div>
                        </div>
                    </form>
                </div>
                <div class="panel-footer">
                    <button id="btnCustUpdateCancel" class="btn btn-link pull-right">Cancel</button>
                    <button id="btnCustUpdateSave" class="btn btn-primary pull-right">Save Changes <span class="fa fa-floppy-o fa-right"></span></button>
                </div>
            </div>

            <div class="panel panel-default" id="divEditPerson">
                <div class="panel-body">
                    <h4>Person details</h4>
                    <!-- PERSON form -->
                    <form class="form-horizontal" action="#" role="form" id="frmPerson">
                        <div class="form-group">
                            <label class="col-md-3 control-label">Full name</label>
                            <div class="col-md-9">
                                <input type="text" class="form-control" id="txtName" placeholder="Full Name" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Email</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-envelope"></i></span>
                                    <input type="text" id="txtEmail" class="form-control" placeholder="Email Address" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Phone</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-phone"></i></span>
                                    <input type="text" id="txtPhone" class="form-control" placeholder="Phone" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Facebook</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-facebook"></i></span>
                                    <input type="text" id="txtFacebook" class="form-control" placeholder="Facebook" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Skype</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-skype"></i></span>
                                    <input type="text" id="txtSkype" class="form-control" placeholder="Skype" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Twitter</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-twitter"></i></span>
                                    <input type="text" id="txtTwitter" class="form-control" placeholder="Twitter" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Govt. Code</label>
                            <div class="col-md-4">
                                <select id="selGovtCode" class="form-control select"></select>
                            </div>
                            <div class="col-md-5">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-rocket"></i></span>
                                    <input id="txtGovtCode" type="text" class="form-control" placeholder="Govt Code" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Photo</label>
                            <div class="col-md-9">
                                <img id="imgPerson" src="../assets/images/users/no-image.jpg" style="width:100px; height:100px;" />
                                <input id="filePhoto" type="file" class="fileinput" name="filename1" id="filename1" />
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">Birth date</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    <input type="text" id="txtDateBirth" class="form-control datepicker" value="" data-date="" data-date-format="dd-mm-yyyy" />
                                    <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Anniversary date</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    <input type="text" id="txtDateAnniversary" class="form-control datepicker" value="" data-date="" data-date-format="dd-mm-yyyy" data-date-viewmode="months" />
                                    <span class="input-group-addon add-on"><span class="glyphicon glyphicon-calendar"></span></span>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Gender</label>
                            <div class="col-md-9">
                                <label class="check"><input type="radio" class="iradio" name="iradioGender" checked="checked" value="0" /> Not set</label>
                                <label class="check"><input type="radio" class="iradio" name="iradioGender" value="1" /> Male</label>
                                <label class="check"><input type="radio" class="iradio" name="iradioGender" value="2" /> Female</label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Maritial status</label>
                            <div class="col-md-9">
                                <label class="check"><input type="radio" class="iradio" name="iradioMStatus" checked="checked" value="0" /> Not set</label>
                                <label class="check"><input type="radio" class="iradio" name="iradioMStatus" value="1" /> Married</label>
                                <label class="check"><input type="radio" class="iradio" name="iradioMStatus" value="2" /> Not Married</label>
                                <label class="check"><input type="radio" class="iradio" name="iradioMStatus" value="3" /> Widow</label>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="panel-footer">
                    <button id="btnCustUpdateCancel" class="btn btn-link pull-right">Cancel</button>
                    <button id="btnCustUpdateSave" class="btn btn-primary pull-right">Save Changes <span class="fa fa-floppy-o fa-right"></span></button>
                </div>
            </div>
        </div>
    </div>
    <!-- PAGE CONTENT - ENDS -->
</div>