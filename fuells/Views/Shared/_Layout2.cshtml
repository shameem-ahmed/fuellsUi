        <script type="text/javascript">

            $(function () {

                if (fuLib.user.jwtToken == null) {
                    window.location.replace("/home/login");
                    return;
                }

                var crPage = $("#hidSelMenu").val();

                fuLib.navigation.load(crPage);

                //alert(crPage);

                if (crPage == 'liPageUser') {
                    doUser();

                }
                else if (crPage == 'liPageSupplier') {
                    doSupplier();

                }
                else if (crPage == 'liPageCompany') {
                    doCompany();

                }
                else if (crPage == 'liPageCustomer') {
                    doCustomer();

                }
                else if (crPage == 'liPageStyle') {
                    doStyle();

                }
                else if (crPage == 'liPageLov') {
                    doLov();

                }
                else if (crPage == 'liPageLoc') {
                    doGloc();

                }
                else if (crPage == 'liPagePO') {
                    doPurchaseOrder();

                }
                else if (crPage == 'liPageLO') {
                    doLeatherOrder();

                }
                else if (crPage == 'liPageJC') {
                    doJobCard();

                }

                $("#printJCTemplate").hide();

            });

            function handleError(action, xhr, status, error) {
                try {

                    console.log(xhr);
                    console.log(status);
                    console.log(error);

                    var err = eval("(" + $(xhr.responseText).text() + ")");

                    noty({ text: err.message, layout: 'topRight', type: 'error', timeout: 2000 });

                }
                catch(err) {

                    noty({ text: action + ' failed.', layout: 'topRight', type: 'error', timeout: 2000 });
                }
            }

            function sleep(milliseconds) {
                var start = new Date().getTime();

                for (i = 0; i < 1e7; i++) {
                    if ((new Date().getTime() - start) > milliseconds) {
                        break;
                    }
                }
            }

            function formatDate(date1) {

                var date = new Date(date1);

                var monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];

                var day = date.getDate();
                var monthIndex = date.getMonth();
                var year = date.getFullYear();

                return day + '-' + monthNames[monthIndex] + '-' + year;
            }

        </script>
    
